{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/prop-types": "off",
        "import/no-extraneous-dependencies": "off",
        // "plugin:react/jsx-runtime": "on",
        "react/react-in-jsx-scope" : "off"
    }
}